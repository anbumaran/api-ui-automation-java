<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd">

<suite name="uiSanitySuite" verbose="1">
    <listeners>
        <listener class-name="com.asapp.common.listener.TestListener"/>
    </listeners>
    <test name="uiSanityTest" parallel="classes" thread-count="4">
        <packages>
            <package name="com.asapp.ui.sanity.*"/>
        </packages>
    </test>
</suite>